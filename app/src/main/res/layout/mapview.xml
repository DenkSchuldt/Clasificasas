<?xml version="1.0" encoding="utf-8"?>


<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:name="com.google.android.gms.maps.SupportMapFragment">


    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/usuario_texto"
        android:textSize="30dp"
        android:id="@+id/textView" />

    <Button android:id="@+id/usuario_publicar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/usuario_publicar"
        android:layout_marginTop="390dip"/>

    <Button android:id="@+id/usuario_buscar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/usuario_buscar"
        android:layout_marginTop="440dip"/>

</fragment>